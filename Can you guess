#include<bits/stdc++.h>
using namespace std;
#define ll      long long int
int solve(int x)
{
    int i,sum=0;
    for(i=1;i<=x/2;i++)
    { if(x%i==0)
        sum+=i;


    }
     cout<<sum<<endl;

}
int main(){
        int i,j;
        cin>>i;
        while(i--){
                cin>>j;

                solve(j);
        }
        return 0;
}
