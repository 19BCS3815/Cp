#include<bits/stdc++.h>
using namespace std;
#define ll      long long int
int solve(int x)
{
    int r,t,sum=0,i=0;
    while(x>=1){
    r=x%9;
    t=r*pow(10,i);
    sum+=t;
     i++;
     x=x/9;
     }
     cout<<sum<<endl;

}
int main(){
        int i;
        cin>>i;
        if(i<9)
        cout<<i<<endl;
        else
        solve(i);

        return 0;
}
